sens-obs:Observation_WindSpeed_1
      a       weather:WindObservation ;
      om-owl:result sens-obs:MeasureData_WindSpeed_1 ;
      om-owl:samplingTime sens-obs:Instant_1 .
      
sens-obs:MeasureData_WindSpeed_1
      a       om-owl:MeasureData ;
      om-owl:floatValue "3.0"^^xsd:float .
      
sens-obs:Instant_1
      a       owl-time:Instant ;
      owl-time:inXSDDateTime
              "18-10-2005T22:30:00^^http://www.w3.org/2001/XMLSchema#dateTime" .
              
sens-obs:Observation_WindGust_2
      a       weather:WindObservation ;
      om-owl:result sens-obs:MeasureData_WindGust_2 ;
      om-owl:samplingTime sens-obs:Instant_2 .
      
sens-obs:MeasureData_WindGust_2
      a       om-owl:MeasureData ;
      om-owl:floatValue "0.0"^^xsd:float .
      
sens-obs:Instant_2
      a       owl-time:Instant ;
      owl-time:inXSDDateTime
              "18-10-2005T07:30:00^^http://www.w3.org/2001/XMLSchema#dateTime" .